#AT7519+OSMI2 transcriptomics data

AT_OSMI_Phospho_Sig <- ggplot(data=de_AT_OSMI, aes(x=log2_FC, y=-log10(P.value)))+
  geom_point(aes(color="grey", size=1.5, alpha=0.4)) + 
  labs(y=expression('-Log'[10]*' P-Value'), x=expression('Log'[2]*' fold change')) +
  theme(legend.position="none", plot.title = element_text(size = rel(1.5), hjust = 0.5))+
  geom_text_repel(data = subset(de_AT_OSMI, GeneID %in% c("EIF4EBP1","LRP6","WEE1","STAT3")), aes(label = GeneID, color = "red"))+
  geom_point(data= subset(de_AT_OSMI, GeneID %in% c("EIF4EBP1","LRP6","WEE1","STAT3")),aes(x=log2_FC, y=-log10(P.value),color="red", size=1.5,alpha=0.4))+
  scale_color_manual(values=c('gray','red'))+ ggtitle("Phospho proteins overlap with transcriptomics; Significant;pvalue < 0.05")

AT_OSMI_TP_Sig <- ggplot(data=de_AT_OSMI, aes(x=log2_FC, y=-log10(P.value)))+
  geom_point(aes(color="grey", size=1.5, alpha=0.4)) + 
  labs(y=expression('-Log'[10]*' P-Value'), x=expression('Log'[2]*' fold change')) +
  theme(legend.position="none", plot.title = element_text(size = rel(1.5), hjust = 0.5))+
  geom_text_repel(data = subset(de_AT_OSMI, GeneID %in% c("ARAF","PPIF","HES1","SMAD3","WEE1")), aes(label = GeneID, color = "red"))+
  geom_point(data= subset(de_AT_OSMI, GeneID %in% c("ARAF","PPIF","HES1","SMAD3","WEE1")),aes(x=log2_FC, y=-log10(P.value),color="red", size=1.5,alpha=0.4))+
  scale_color_manual(values=c('gray','red'))+ ggtitle("Total proteins overlap with transcriptomics AT+OSMI2 (Significant, pvalue < 0.05)")


