#de <- read.table("D:\\MultiOmics\\DEGs_RPPA_OSMI_AT_ForVolcano.txt", header = TRUE, sep = "\t")
de <- read.table("D:\\MultiOmics\\DEGs_RPPA_OSMI_AT_PhospProtein_Proteins_ForVolcano.txt", header = TRUE, sep = "\t")
de_AT_OSMI <- read.table("D:\\MultiOmics\\DEGs_RPPA_OSMI+AT_PhospProtein_Proteins_ForVolcano.txt", header = TRUE, sep = "\t")


p4 <- ggplot(data=de, aes(x=log2_FC_, y=-log10(P.value)))+
geom_point(aes(color="grey", size=1.5, alpha=0.4)) + 
labs(y=expression('-Log'[10]*' P-Value'), x=expression('Log'[2]*' fold change')) +
theme(legend.position="none", plot.title = element_text(size = rel(1.5), hjust = 0.5))+
#geom_text_repel(data  = subset(de_1, GeneID %in% list4), aes(label = GeneID, color = "red"))+
geom_point(data = subset(de, GeneID %in% List1_down),aes(x=log2_FC_, y=-log10(P.value),color="red", size=1.5,alpha=0.4))+
#scale_color_manual(values=c('gray','red'))+
geom_point(data = subset(de, GeneID %in% List2_Up),aes(x=log2_FC_, y=-log10(P.value),color="blue", size=1.5,alpha=0.4))+
geom_point(data = subset(de, GeneID %in% List3_down_unique),aes(x=log2_FC_, y=-log10(P.value),color="black", size=0.5,alpha=0.4))+
geom_point(data = subset(de, GeneID %in% List4_up_unqiue),aes(x=log2_FC_, y=-log10(P.value),color="black", size=0.5,alpha=0.4))+
scale_color_manual(values=c('black','red','gray','blue'))


p5 <- ggplot(data=de, aes(x=log2_FC_, y=-log10(P.value)))+
  geom_point(aes(color="grey", size=1.5, alpha=0.4)) + 
  labs(y=expression('-Log'[10]*' P-Value'), x=expression('Log'[2]*' fold change')) +
  theme(legend.position="none", plot.title = element_text(size = rel(1.5), hjust = 0.5))+
  #geom_text_repel(data  = subset(de_1, GeneID %in% list4), aes(label = GeneID, color = "red"))+
  geom_point(data = subset(de, GeneID %in% List1_down),aes(x=log2_FC_, y=-log10(P.value),color="red", size=1.5,alpha=0.4))+
  #scale_color_manual(values=c('gray','red'))+
  geom_point(data = subset(de, GeneID %in% List2_Up),aes(x=log2_FC_, y=-log10(P.value),color="blue", size=1.5,alpha=0.4))+
  geom_point(data = subset(de, GeneID %in% List3_down_unique),aes(x=log2_FC_, y=-log10(P.value),color="black", size=1,alpha=0.4))+
  geom_point(data = subset(de, GeneID %in% List4_up_unqiue),aes(x=log2_FC_, y=-log10(P.value),color="black", size=1,alpha=0.4))+
  scale_color_manual(values=c('black','red','gray','blue'))


p6 <- ggplot(data=de, aes(x=log2_FC, y=-log10(P.value)))+
  geom_point(aes(color="grey", size=1.5, alpha=0.4)) + 
  labs(y=expression('-Log'[10]*' P-Value'), x=expression('Log'[2]*' fold change')) +
  theme(legend.position="none", plot.title = element_text(size = rel(1.5), hjust = 0.5))+
  #geom_text_repel(data  = subset(de_1, GeneID %in% list4), aes(label = GeneID, color = "red"))+
  geom_point(data = subset(de, GeneID %in% List5),aes(x=log2_FC, y=-log10(P.value),color="red", size=1.5,alpha=0.4))+
  scale_color_manual(values=c('gray','red'))
  
p7_Phospho <- ggplot(data=de, aes(x=log2_FC, y=-log10(P.value)))+
  geom_point(aes(color="grey", size=1.5, alpha=0.4)) + 
  labs(y=expression('-Log'[10]*' P-Value'), x=expression('Log'[2]*' fold change')) +
  theme(legend.position="none", plot.title = element_text(size = rel(1.5), hjust = 0.5))+
  #geom_text_repel(data  = subset(de_1, GeneID %in% list4), aes(label = GeneID, color = "red"))+
  geom_point(data = subset(de, GeneID %in% Gene.Name_PhosphoProteins),aes(x=log2_FC, y=-log10(P.value),color="red", size=1.5,alpha=0.4))+
  scale_color_manual(values=c('gray','red'))+ ggtitle("Phospho proteins overlap with transcriptomics")

p7_TotalProtein <- ggplot(data=de, aes(x=log2_FC, y=-log10(P.value)))+
  geom_point(aes(color="grey", size=1.5, alpha=0.4)) + 
  labs(y=expression('-Log'[10]*' P-Value'), x=expression('Log'[2]*' fold change')) +
  theme(legend.position="none", plot.title = element_text(size = rel(1.5), hjust = 0.5))+
  #geom_text_repel(data  = subset(de_1, GeneID %in% list4), aes(label = GeneID, color = "red"))+
  geom_point(data = subset(de, GeneID %in% Gene.Name_Proteins),aes(x=log2_FC, y=-log10(P.value),color="red", size=1.5,alpha=0.4))+
  scale_color_manual(values=c('gray','red'))+ ggtitle("Total proteins overlap with transcriptomics")

