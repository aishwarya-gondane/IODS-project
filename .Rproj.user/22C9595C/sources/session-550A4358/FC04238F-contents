#AT7519 transcriptomics data

p7_Phospho_Sig <- ggplot(data=de, aes(x=log2_FC, y=-log10(P.value)))+
  geom_point(aes(color="grey", size=1.5, alpha=0.4)) + 
  labs(y=expression('-Log'[10]*' P-Value'), x=expression('Log'[2]*' fold change')) +
  theme(legend.position="none", plot.title = element_text(size = rel(1.5), hjust = 0.5))+
  geom_text_repel(data = subset(de, GeneID %in% c ("GSK3A","GSK3B", "LRP6", "PRKCA2", "PRKCB" ,"PRKCD", "PRKCE" , "PRKCH", "PRKCQ", "RPS6", "WEE1", "AKT1", "SHC1")), aes(label = GeneID, color = "red"))+
  geom_point(data= subset(de, GeneID %in% c ("GSK3A","GSK3B", "LRP6", "PRKCA2", "PRKCB" ,"PRKCD", "PRKCE" ,"PRKCH", "PRKCQ", "RPS6", "WEE1", "AKT1", "SHC1")),aes(x=log2_FC, y=-log10(P.value),color="red", size=1.5,alpha=0.4))+
  scale_color_manual(values=c('gray','red'))+ ggtitle("Phospho proteins overlap with transcriptomics AT7519 data (Significant;pvalue > 0.05)")

p7_TotalProtenSig <- ggplot(data=de, aes(x=log2_FC, y=-log10(P.value)))+
  geom_point(aes(color="grey", size=1.5, alpha=0.4)) + 
  labs(y=expression('-Log'[10]*' P-Value'), x=expression('Log'[2]*' fold change')) +
  theme(legend.position="none", plot.title = element_text(size = rel(1.5), hjust = 0.5))+
  geom_text_repel(data  = subset(de, GeneID %in% c ("ARAF, BRAF", "BRD4", "GLUD", "MCL1", "MMP2","SDHA" )), aes(label = GeneID, color = "red"))+
  geom_point(data = subset(de, GeneID %in% c ("ARAF, BRAF", "BRD4", "GLUD", "MCL1", "MMP2","SDHA" )),aes(x=log2_FC, y=-log10(P.value),color="red", size=1.5,alpha=0.4))+
  scale_color_manual(values=c('gray','red'))+ ggtitle("Total proteins overlap with transcriptomics AT7519 data (Significant, pvalue < 0.05)")

