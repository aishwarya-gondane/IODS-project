#OSMI2 transcriptomics data

de_OSMI <- read.table("D:\\MultiOmics\\DEGs_RPPA_OSMI_PhospProtein_Proteins_ForVolcano.txt", header = TRUE, sep = "\t")

OSMI_Phospho_Sig <- ggplot(data=de_OSMI, aes(x=log2_FC, y=-log10(P.value)))+
  geom_point(aes(color="grey", size=1.5, alpha=0.4)) + 
  labs(y=expression('-Log'[10]*' P-Value'), x=expression('Log'[2]*' fold change')) +
  theme(legend.position="none", plot.title = element_text(size = rel(1.5), hjust = 0.5))+
  geom_text_repel(data = subset(de_OSMI, GeneID %in% c("EIF4EBP1","BAD","MDM2","YBX1")), aes(label = GeneID, color = "red"))+
  geom_point(data= subset(de_OSMI, GeneID %in% c("EIF4EBP1","BAD","MDM2","YBX1")),aes(x=log2_FC, y=-log10(P.value),color="red", size=1.5,alpha=0.4))+
  scale_color_manual(values=c('gray','red'))+ ggtitle("Phospho proteins overlap with transcriptomics OSMI2 data; Significant;pvalue < 0.05")

OSMI_TP_Sig <- ggplot(data=de_OSMI, aes(x=log2_FC, y=-log10(P.value)))+
  geom_point(aes(color="grey", size=1.5, alpha=0.4)) + 
  labs(y=expression('-Log'[10]*' P-Value'), x=expression('Log'[2]*' fold change')) +
  theme(legend.position="none", plot.title = element_text(size = rel(1.5), hjust = 0.5))+
  geom_text_repel(data = subset(de_OSMI, GeneID %in% c("ACACA","ACACB","AR","CTNNB1","MYC","CASP3","E2F1","EIF4E","LCK","MIF","NAPSA","RAD51","TRIM25")), aes(label = GeneID, color = "red"))+
  geom_point(data= subset(de_OSMI, GeneID %in% c("ACACA","ACACB","AR","CTNNB1","MYC","CASP3","E2F1","EIF4E","LCK","MIF","NAPSA","RAD51","TRIM25")),aes(x=log2_FC, y=-log10(P.value),color="red", size=1.5,alpha=0.4))+
  scale_color_manual(values=c('gray','red'))+ ggtitle("Total proteins overlap with transcriptomics OSMI2 data (Significant, pvalue < 0.05)")
